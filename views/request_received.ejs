<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"  href="css/request_recieved.css" type="text/css">
    <title>Requests</title>
</head>
<body>
   
    <div class="container">
       <% for(  i in arr2 ) { %>
    <div class="child">
         <P id="nme"> <%= arr2[i] %></P>
        <button type="submit" class="btn" id="tick"  onclick="tick( '<%- arr2[i] %>' )"></button>
        <button type="submit" class="btn" id="cross" onclick="cross('<%- arr2[i] %>'  )"></button>
    </div>
    <% } %>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
        let curr = localStorage.getItem("user");
        function tick(user){
            console.log(user +" tick");
            $.post("/doOnrequest",{acc: curr, friend: user, status: "accepted"}, function(data,status){
                
            });

        };
       function cross(user){
            console.log(user +" cross");
            $.post("/doOnrequest",{acc: curr, friend: user, status: "rejected"}, function(data,status){
                
            });
        };
    </script>
    
</body>
</html>
