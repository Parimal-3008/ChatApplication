    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"  href="css/request_recieved.css" type="text/css">
    <title>Requests</title>
</head>
<body>
   
    <div class="container">
       <% for(  i in arr2 ) { %>
    <div class="child">
         <P id="nme"> <%= arr2[i] %></P>
        <button type="submit" class="btn" id="tick"  onclick="tick( '<%- arr2[i] %>' )"></button>
        <button type="submit" class="btn" id="cross" onclick="cross('<%- arr2[i] %>'  )"></button>
    </div>
    <% } %>
    </div>
    <script>
        function tick(user){
            console.log(user +" tick");
        };
       function cross(user){
            console.log(user +" cross");
        };
    </script>
    
</body>
</html>
