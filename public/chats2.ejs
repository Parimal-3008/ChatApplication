<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet"  href="css/chats.css" type="text/css">
    <title>Chat Application</title>
  </head>
  
  <body>
   
  <div class="parent">
    <div class="left">
      <div class="container">
        
          <form action="/searchforuser" method="get">  
          <button type="submit" class="send" onclick="toSearch">
          Search for Friends
        </button>
        </form>
     
      <form action="/request_received" method="get">  
        <button type="submit" class="recieve" onclick="toRequest">
            Requests recieved
        </button>
        </form>
      </div>
      <div class="pt2">
  
      </div>
    </div>
    <div class="space"></div>
    <div class="right">
      <div class="name"></div>
      <div class="msg"></div>
      <div class="inputMsg">
        <input type="text" placeholder="Type your message here" >
        <button type="submit" class="sendButton"><img src="css/send.png"></button>
      </div>
      

    </div>
  </div>
    <script src="/socket.io/socket.io.js"></script>
    <script  src="event.js"> </script> 
    
  </body>
</html>
